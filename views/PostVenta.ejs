            <div class="tab-content" id="myTabContent">
               <div class="tab-pane fade show active" id="list" role="tabpanel" aria-labelledby="list-tab">
                  <div class="card">
                  <div class="card-header">
                        <h4>Servicios a Facturar</h4>
                     </div>
                     <div class="card-body">
                        <div class="table-responsive">
                           <table id="userList" class="table table-bordered table-hover table-striped">
                              <thead class="thead-light">
                              <tr>
                                 <th class="centered">servicio id</th>
                                 <th class="centered">Contrato id</th>
                                 <th class="centered">nivel servicio </th>
                                 <th class="centered">Visitas</th>
                                 <th class="centered">Precio</th>
                                 <th class="centered">Fecha</th>
                                 <th class="centered">facturar</th>
                              </tr>
                              </thead>
          
                                 <tbody id="tableBody_users">
                             
                                    <% resultado.forEach((servicio) => { %>
                                <tr>
                                 <td class="centered"><%= servicio.servicio_id %></td>
                                 <td class="centered"><%= servicio.contrato_id %></td>
                                 <td class="centered"><%= servicio.nivel_servicio %></td>
                                 <td class="centered"><%= servicio.cantidad_visitas %></td>
                                 <td class="centered"><%= servicio.precio %></td>
                                 <td class="centered"><%= servicio.fecha %></td>
                                 
                                 <td>
                                    <a href="/postventa/<%= servicio.precio %>/<%= servicio.servicio_id %>"><i class="fas fa-edit" id="editar"></i></a>
                                 </td> 
                                </tr> 
                                  
                                <% }); %>
                                   
                                </tbody> 
                           </table>
                        </div>
                     </div>
                  </div>
               </div>
               <div class="tab-pane fade" id="form" role="tabpanel" aria-labelledby="form-tab">
