                              <tr>
                                 <th class="centered">servicio id</th>
                                 <th class="centered">Contrato id</th>
                                 <th class="centered">nivel servicio </th>
                                 <th class="centered">Visitas</th>
                                 <th class="centered">Precio</th>
                                 <th class="centered">Fecha</th>
                                 <th class="centered">factura</th>
                                 <th class="centered">fecha factura</th>
                              </tr>
                              </thead>
          
                                 <tbody id="tableBody_users">
                             
                                    <% facturado.forEach((facturado) => { %>
                                <tr>
                                 <td class="centered"><%= facturado.servicio_id %></td>
                                 <td class="centered"><%= facturado.contrato_id %></td>
                                 <td class="centered"><%= facturado.nivel_servicio %></td>
                                 <td class="centered"><%= facturado.cantidad_visitas %></td>
                                 <td class="centered"><%= facturado.precio %></td>
                                 <td class="centered"><%= facturado.fecha %></td>
                                 <td class="centered"><%= facturado.noFactura %></td>
                                 <td class="centered"><%= facturado.fechaServ %></td>
                                  
                                </tr> 
                                  
                                <% }); %>
                                   
                                </tbody> 
                           </table>
                        </div>
                     </div>
